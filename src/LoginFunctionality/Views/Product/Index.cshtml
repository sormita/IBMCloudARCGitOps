@model IEnumerable<Product>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Product</h1>
<div class="row">
    <ul>
        @foreach (var prod in Model)
        {
        <li>@Html.DisplayFor(modelItem => prod.Name) <br /> @Html.DisplayFor(modelItem => prod.ProductNumber)   </li>
            
        }
    </ul>
</div>


